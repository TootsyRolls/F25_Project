<BorderPane style="-fx-border-color: Black; -fx-border-width: 3;" stylesheets="@../Resources/Style.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="f25_project.Controller.MainTableController">
   <center>
      <Pane fx:id="table" prefHeight="800.0" prefWidth="600.0" style="-fx-border-color: black; -fx-border-width: 3;" stylesheets="@../Resources/Style.css">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Circle fx:id="cueBall" fill="DODGERBLUE" layoutX="100.0" layoutY="400.0" radius="25.0" stroke="BLACK" strokeType="INSIDE" />
            <Circle fill="DODGERBLUE" layoutX="549.0" layoutY="400.0" radius="27.0" stroke="BLACK" strokeType="INSIDE" />
         </children>
         <BorderPane.margin>
            <Insets bottom="-2.0" left="-2.0" right="-2.0" top="-2.0" />
         </BorderPane.margin>
      </Pane>
   </center>
   <top>
      <MenuBar prefWidth="606.0" style="-fx-background-color: lightGray; -fx-border-color: black;" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="Tab">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
         </menus>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </MenuBar>
   </top>
   <left>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="230.0" spacing="25.0" style="-fx-background-color: #c9c9c9; -fx-border-color: black;" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </padding>
         <children>
            <HBox alignment="CENTER_LEFT" spacing="25.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="143.0" spacing="10.0">
                     <children>
                        <Label id="label-data" alignment="CENTER" prefHeight="25.0" prefWidth="75.0" text="Friction">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label id="label-data" fx:id="frictionLabel" alignment="CENTER" contentDisplay="TOP" text="Label" />
                     </children>
                  </VBox>
                  <Slider fx:id="frictionSlider" majorTickUnit="0.1" max="1.0" min="0.1" minorTickCount="5" orientation="VERTICAL" prefHeight="250.0" showTickLabels="true" showTickMarks="true" style="-: ;">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </Slider>
               </children>
               <padding>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefWidth="218.0" spacing="25.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="143.0" spacing="10.0">
                     <children>
                        <Label id="label-data" alignment="CENTER" prefHeight="25.0" prefWidth="75.0" text="Mass">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                        <Label id="label-data" fx:id="massLabel" alignment="CENTER" contentDisplay="TOP" text="Label" />
                     </children>
                  </VBox>
                  <Slider fx:id="massSlider" majorTickUnit="4.0" max="25.0" min="1.0" minorTickCount="5" orientation="VERTICAL" prefHeight="250.0" showTickLabels="true" showTickMarks="true" style="-: ;">
